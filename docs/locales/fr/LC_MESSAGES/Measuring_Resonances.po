"serial : /dev/serial/by-id/<le serial id du PICO>\n"
"\n"
"[mpu9250]\n"
"i2c_mcu : pico\n"
"i2c_bus : i2c1a\n"
"\n"
"[resonance_tester]\n"
"accel_chip : mpu9250\n"
"probe_points :\n"
"    100, 100, 20  # un exemple\n"
"\n"
"[static_digital_output pico_3V3pwm] # Amélioration de la stabilité de l'"
"alimentation\n"
"pin : pico :gpio23\n"

