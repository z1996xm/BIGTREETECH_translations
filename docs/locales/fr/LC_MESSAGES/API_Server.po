msgstr "<json_object_1><0x03><json_object_2><0x03>...\n"

msgid "~/klipper/scripts/whconsole.py /tmp/klippy_uds\n"
msgstr "~/klipper/scripts/whconsole.py /tmp/klippy_uds\n"

